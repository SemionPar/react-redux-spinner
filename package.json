{
  "name": "react-redux-spinner",
  "version": "0.7.0",
  "description": "A spinner and loading bar for any long running tasks. For react with redux",
  "main": "dist/react-redux-spinner.js",
  "scripts": {
    "example:run": "NODE_ENV=development webpack-dev-server --config example/webpack.config.babel.js",
    "example:build": "NODE_ENV=production webpack --config example/webpack.config.babel.js",
    "lint": "eslint src example test",
    "mocha": "mocha --compilers js:babel-core/register",
    "test": "yarn mocha && yarn lint",
    "build": "NODE_ENV=production webpack --config src/webpack.config.babel.js",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Storytel/react-redux-spinner.git"
  },
  "keywords": [
    "react",
    "redux",
    "spinner",
    "loadingbar",
    "ajax"
  ],
  "author": "Alexander Olsson <noseglid@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Storytel/react-redux-spinner/issues"
  },
  "homepage": "https://github.com/Storytel/react-redux-spinner#readme",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-storytel": "^3.1.1",
    "eslint-plugin-react": "^6.10.3",
    "expect.js": "^0.3.1",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.12.0",
    "mocha": "^3.2.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.5.6",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "redux": "^3.3.1",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}
